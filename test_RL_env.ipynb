    "import gymnasium as gym\n",
    "from stable_baselines3 import SAC\n",
    "import pendulum_env\n",
    "\n",
    "env = pendulum_env.MuJoCoPendulumEnv()\n",
    "\n",
    "model = SAC(\n",
    "    \"MlpPolicy\",\n",
    "    env,\n",
    "    learning_rate=3e-4,\n",
    "    gamma=0.99,\n",
    "    verbose=1,\n",
    ")\n",
    "\n",
    "model.learn(total_timesteps=50_000)\n"
   ]
